    <string name="fourth_question_second_answer">טיפול פסיכיאטרי</string>
    <string name="fourth_question_third_answer">טיפול הורמונלי</string>
    <string name="fourth_question_fourth_answer">נכון להיום לא קיימת דרך לרפא אנדומטריוזיס</string>
    <string name="fifth_question">5. האם מופיעים סימפטומים זהים בכל מקרי האנדומטריוזיס?</string>
    <string name="fifth_question_first_answer">כן. תמיד ניתן להסתמך על סימפטומים של בן-אדם אחר בשביל אבחון עצמי</string>
    <string name="fifth_question_second_answer">לא. יתכנו מקרים קשים ללא סימפטומים כלל, ויתכנו מקרים קלים אך עם סימפטומים חמורים</string>
    <string name="fifth_question_third_answer">תלוי באיזה סוג של סימפטומים מדובר</string>
    <string name="fifth_question_fourth_answer">לא. באנדומטריוזיס הסימפטומים הם פסיכוסומטיים ולכן משתנים בין מקרה למקרה, כתלות במצב הנפשי</string>
    <string name="sixth_question">6. מה זה אדנומיוזיס?</string>
    <string name="sixth_question_first_answer">שיקוי יווני עתיק, שהמתכון שלו נמצא על ידי ארכיאולוגים ביחד עם רשומות המתעדות את צריכתו להקלת סימפטומי אנדומטריוזיס</string>
    <string name="sixth_question_second_answer">השלב הראשון בהתפתחות אנדומטריוזיס</string>
    <string name="sixth_question_third_answer">אנדומטריוזיס בתוך המיומטריום (שריר הרחם)</string>
    <string name="sixth_question_fourth_answer">רקמת הצלקת שנותרת לאחר ריפוי האנדומטריוזיס</string>
